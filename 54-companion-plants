public Boolean companionPlants(String plant1, String plant2) {
    //code here

    Set<Set<String>> companions = new Set<Set<String>>();
    companions.add(new Set<String>{'lettuce', 'cucumbers'});
    companions.add(new Set<String>{'lettuce', 'onions'});
    companions.add(new Set<String>{'onions', 'carrots'});
    companions.add(new Set<String>{'onions', 'tomatoes'});
    
    // Check if the given plant pair is a companion
    Set<String> plantPair = new Set<String>{plant1, plant2};
    return companions.contains(plantPair);


}